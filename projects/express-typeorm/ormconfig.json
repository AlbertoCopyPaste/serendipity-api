{
  "type": "sqlite",
  "database": "serendipity.db",
  "synchronize": true,
  "logging": false,
  "entities": [
    "src/api/models/**/*.ts"
  ],
  "migrations": [
    "src/database/migrations/**/*.ts"
  ],
  "subscribers": [
    "src/api/subscribers/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/api/models",
    "migrationsDir": "src/database/migrations",
    "subscribersDir": "src/api/subscribers"
  }
}
